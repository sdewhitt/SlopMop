# Firebase configuration for the SlopMop extension.
# Copy this file to .env and fill in the values from the Firebase console.
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=

# Google OAuth Client ID for extension Google Sign-In
# Get this from Firebase Console > Authentication > Sign-in method > Google > Web SDK configuration > Web client ID
VITE_FIREBASE_OAUTH_CLIENT_ID=

# SlopMop backend API (Render deployment URL).
# Production: https://your-service-name.onrender.com
VITE_API_BASE_URL=
